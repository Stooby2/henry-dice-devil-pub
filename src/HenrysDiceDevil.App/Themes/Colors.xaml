<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Color.Background">#11171F</Color>
    <Color x:Key="Color.Surface">#192331</Color>
    <Color x:Key="Color.SurfaceRaised">#223246</Color>
    <Color x:Key="Color.SurfaceSubtle">#1A2431</Color>
    <Color x:Key="Color.Border">#33485F</Color>
    <Color x:Key="Color.BorderStrong">#4A6A89</Color>
    <Color x:Key="Color.Accent">#2CCCB6</Color>
    <Color x:Key="Color.AccentHover">#52D8C5</Color>
    <Color x:Key="Color.AccentPressed">#1FA997</Color>
    <Color x:Key="Color.Warning">#E6A94A</Color>
    <Color x:Key="Color.Danger">#E26A6A</Color>
    <Color x:Key="Color.TextPrimary">#E6F0FA</Color>
    <Color x:Key="Color.TextSecondary">#A8BCD1</Color>
    <Color x:Key="Color.TextMuted">#7C93AD</Color>

    <SolidColorBrush x:Key="Brush.Background" Color="{StaticResource Color.Background}" />
    <SolidColorBrush x:Key="Brush.Surface" Color="{StaticResource Color.Surface}" />
    <SolidColorBrush x:Key="Brush.SurfaceRaised" Color="{StaticResource Color.SurfaceRaised}" />
    <SolidColorBrush x:Key="Brush.SurfaceSubtle" Color="{StaticResource Color.SurfaceSubtle}" />
    <SolidColorBrush x:Key="Brush.Border" Color="{StaticResource Color.Border}" />
    <SolidColorBrush x:Key="Brush.BorderStrong" Color="{StaticResource Color.BorderStrong}" />
    <SolidColorBrush x:Key="Brush.Accent" Color="{StaticResource Color.Accent}" />
    <SolidColorBrush x:Key="Brush.AccentHover" Color="{StaticResource Color.AccentHover}" />
    <SolidColorBrush x:Key="Brush.AccentPressed" Color="{StaticResource Color.AccentPressed}" />
    <SolidColorBrush x:Key="Brush.Warning" Color="{StaticResource Color.Warning}" />
    <SolidColorBrush x:Key="Brush.Danger" Color="{StaticResource Color.Danger}" />
    <SolidColorBrush x:Key="Brush.TextPrimary" Color="{StaticResource Color.TextPrimary}" />
    <SolidColorBrush x:Key="Brush.TextSecondary" Color="{StaticResource Color.TextSecondary}" />
    <SolidColorBrush x:Key="Brush.TextMuted" Color="{StaticResource Color.TextMuted}" />

    <!-- MaterialDesignInXamlToolkit combo surface theming (template brush keys). -->
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.Popup.DarkBackground" Color="{StaticResource Color.Surface}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.Popup.DarkForeground" Color="{StaticResource Color.TextPrimary}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.Popup.LightBackground" Color="{StaticResource Color.Surface}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.Popup.LightForeground" Color="{StaticResource Color.TextPrimary}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.FilledBackground" Color="{StaticResource Color.SurfaceSubtle}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.HoverBackground" Color="{StaticResource Color.SurfaceRaised}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.Border" Color="{StaticResource Color.BorderStrong}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.OutlineBorder" Color="{StaticResource Color.BorderStrong}" />
    <SolidColorBrush x:Key="MaterialDesign.Brush.ComboBox.OutlineInactiveBorder" Color="{StaticResource Color.Border}" />

    <SolidColorBrush x:Key="{x:Static SystemColors.WindowBrushKey}" Color="#1A2431" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="#192331" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlLightBrushKey}" Color="#223246" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlDarkBrushKey}" Color="#33485F" />
    <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="#E6F0FA" />
    <SolidColorBrush x:Key="{x:Static SystemColors.WindowTextBrushKey}" Color="#E6F0FA" />

    <LinearGradientBrush x:Key="Brush.WindowGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Offset="0.0" Color="#152130" />
        <GradientStop Offset="0.55" Color="#11171F" />
        <GradientStop Offset="1.0" Color="#0E131A" />
    </LinearGradientBrush>
</ResourceDictionary>
